{
    "manifest_version": 3,
    "name": "Unpaywall",
    "version": "1.0",
    "description": "Adds http://12ft.io/ prefix to the current URL.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Add 12ft.io Prefix"
    }
}
